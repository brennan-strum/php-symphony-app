{% extends 'base.html.twig' %}

{% block title %}Add Album Photo{% endblock %}

{% block body %}
    <div class="flex w-full">
      <div class="image-form-wrapper">
          <div class="form">
              <h1 class="mb-3 font-weight-normal text-2xl">Add Photo</h1>
              {{ form_start(form) }}
                {{ form_widget(form) }}
                <div class="flex justify-end w-full">
                    <button type="submit" class="btn btn-primary">Submit</button>
                </div>
              {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}
